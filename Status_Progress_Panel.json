              {
                "name": "Integritas Sistem",
                "value": "dinamis",
                "source": "lukmanpro_v1.1.get_system_integrity()",
                "description": "Kesehatan sistem secara keseluruhan (0-100%).",
                "current_value": "lukmanpro_v1.1.get_system_integrity()"
