            ]
          },
          {
            "name": "Jejak Dimensi",
            "icon": "⊹",
            "metrics": [
              {
                "name": "Dimensi Aktif",
                "value": "dinamis",
                "source": "dimensional_footprint.get_active_dimensions()",
