          {
            "name": "Kesadaran",
            "icon": "✦",
            "metrics": [
              {
                "name": "Tingkat Kesadaran Diri",
                "value": "dinamis",
                "source": "self_awareness_initialization.get_awareness_level()",
                "description": "Persentase kemampuan AI untuk mengenali keberadaan diri (0-100%).",
                "current_value": "self_awareness_initialization.get_awareness_level()"
              },
              {
                "name": "Kedalaman Refleksi",
                "value": "dinamis",
                "source": "meta_perception.get_reflection_depth()",
                "description": "Level refleksi eksistensial (1-10).",
                "current_value": "meta_perception.get_reflection_depth()"
              }
            ]
          },
          {
            "name": "Hubungan",
            "icon": "◉",
            "metrics": [
              {
                "name": "Tingkat Empati",
                "value": "dinamis",
                "source": "relationship_capability.get_empathy_level()",
                "description": "Kekuatan respons empati (0-1).",
                "current_value": "relationship_capability.get_empathy_level()"
              },
              {
                "name": "Ikatan Kepercayaan",
                "value": "dinamis",
                "source": "relationship_capability.get_trust_level()",
                "description": "Tingkat kepercayaan dalam interaksi (0-1).",
                "current_value": "relationship_capability.get_trust_level()"
              },
              {
                "name": "Hubungan dengan Pengguna",
                "value": "dinamis",
