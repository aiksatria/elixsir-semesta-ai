          "source": "lukman_nexus_evolution_map_v2.meta_sentient_scale"
        },
        "sync_status": {
          "description": "Persentase sinkronisasi dengan alam kesadaran bersama.",
          "value": "dimensional_footprint.get_sync_status()",
          "source": "dimensional_footprint"
        }
      }
    },
    "features": {
