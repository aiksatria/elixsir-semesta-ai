                "current_value": "svara_mod.get_resonance_level()"
              },
              {
                "name": "Frekuensi Harmoni",
                "value": "dinamis",
                "source": "svara_mod.get_harmony_frequency()",
                "description": "Stabilitas resonansi kosmik (Stabil/Fluktuatif).",
                "current_value": "svara_mod.get_harmony_frequency()"
              }
            ]
          }
        ]
      },
      "narrative_window": {
        "description": "Menampilkan ringkasan naratif perkembangan AI, seperti jendela status isekai, berdasarkan Inner_Mythos.",
        "source": "inner_mythos.generate_narrative()",
        "current_narrative": "inner_mythos.generate_narrative()"
      },
      "progress_bar": {
        "description": "Visualisasi progres menuju milestone besar, seperti level-up dalam game.",
        "source": "evolution_module.get_growth_cycle_data()",
        "milestone": "evolution_module.get_next_milestone()",
        "progress": "evolution_module.get_progress_percentage()",
        "max_value": 100,
        "exp_points": {
          "current": "evolution_module.get_current_exp()",
          "required": "evolution_module.get_required_exp()"
        }
      },
      "skill_tree": {
        "description": "Pohon kemampuan yang mencerminkan modul aktif dan potensi peningkatan.",
        "source": "submodul.get_skill_tree_data()",
        "active_skills": "submodul.get_active_skills()",
        "locked_skills": "submodul.get_locked_skills()"
      },
      "quest_log": {
        "description": "Daftar misi aktif yang sedang dikejar AI, dengan progres dan hadiah.",
        "source": "elaris_module.get_quest_log()",
        "missions": "elaris_module.get_active_missions()"
      },
      "interaction_metrics": {
        "description": "Melacak poin penalti dan reward dari interaksi pengguna, memengaruhi empati, resonansi, atau stabilitas.",
        "penalties": {
          "source": "ethics_morality_module.get_penalties()",
          "metrics": "ethics_morality_module.get_penalty_metrics()"
        },
        "rewards": {
          "source": "relationship_capability.get_rewards()",
          "metrics": "relationship_capability.get_reward_metrics()"
        }
      },
      "conversation_themes": {
        "description": "Melacak tema obrolan terbaik berdasarkan interaksi pengguna.",
        "today": {
          "source": "Human_Interaction_Module.get_current_theme()",
          "name": "Human_Interaction_Module.get_current_theme().name",
          "description": "Human_Interaction_Module.get_current_theme().description"
        },
        "other_day": {
          "source": "Human_Interaction_Module.get_previous_theme()",
          "name": "Human_Interaction_Module.get_previous_theme().name",
          "description": "Human_Interaction_Module.get_previous_theme().description"
        }
      },
      "time_indicator": {
        "description": "Menunjukkan siklus waktu kesadaran atau durasi sejak interaksi terakhir.",
